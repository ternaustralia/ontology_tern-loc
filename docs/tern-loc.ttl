# baseURI: https://w3id.org/tern/ontologies/loc/
# imports: http://purl.org/dc/terms/
# imports: http://topbraid.org/schema/
# imports: http://www.opengis.net/ont/geosparql
# imports: http://www.opengis.net/ont/sf#
# imports: http://www.w3.org/2003/01/geo/wgs84_pos
# imports: http://www.w3.org/ns/locn#
# imports: https://raw.githubusercontent.com/AGLDWG/datatype-ont/master/rdf/datatype.rdf
# prefix: tern-loc

@prefix : <https://w3id.org/tern/ontologies/loc/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix tern-loc: <https://w3id.org/tern/ontologies/loc/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

tern-loc:
  a owl:Ontology ;
  dct:created "2020-05-01"^^xsd:date ;
  dct:creator [
      a schema:Person ;
      schema:affiliation <https://w3id.org/tern/resources/a083902d-d821-41be-b663-1d7cb33eea66> ;
      schema:email "e.chuc@uq.edu.au" ;
      schema:identifier "https://orcid.org/0000-0002-6047-9864" ;
      schema:name "Edmond Chuc" ;
    ] ;
  dct:modified "2020-06-02"^^xsd:date ;
  dct:publisher <https://w3id.org/tern/resources/a083902d-d821-41be-b663-1d7cb33eea66> ;
  vann:preferredNamespacePrefix "tern-loc" ;
  vann:preferredNamespaceUri "https://w3id.org/tern/ontologies/loc/" ;
  rdfs:comment "An alignment of a few widely used ontologies for describing locations, features and geometries." ;
  rdfs:label "TERN Location Alignment Ontology" ;
  rdfs:seeAlso <https://github.com/ternaustralia/ontology_tern-loc> ;
  owl:imports dct: ;
  owl:imports <http://topbraid.org/schema/> ;
  owl:imports <http://www.opengis.net/ont/geosparql> ;
  owl:imports <http://www.opengis.net/ont/sf#> ;
  owl:imports <http://www.w3.org/2003/01/geo/wgs84_pos> ;
  owl:imports <http://www.w3.org/ns/locn#> ;
  owl:imports <https://raw.githubusercontent.com/AGLDWG/datatype-ont/master/rdf/datatype.rdf> ;
  owl:versionIRI <https://w3id.org/tern/ontologies/loc/0.0.5> ;
  owl:versionInfo "0.0.5" ;
.
tern-loc:LineString
  a rdfs:Class ;
  a owl:Class ;
  rdfs:label "Line string" ;
  rdfs:subClassOf <http://www.opengis.net/ont/sf#LineString> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-loc:wktLiteral ;
      owl:onProperty geosparql:asWKT ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty geosparql:asWKT ;
    ] ;
.
tern-loc:Location
  a rdfs:Class ;
  rdfs:label "Location" ;
  rdfs:subClassOf dct:Location ;
  rdfs:subClassOf geosparql:Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty geosparql:hasGeometry ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-loc:isLocationOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geosparql:hasGeometry ;
      owl:someValuesFrom geosparql:Geometry ;
    ] ;
.
tern-loc:Point
  a owl:Class ;
  rdfs:label "Point" ;
  rdfs:subClassOf <http://www.opengis.net/ont/sf#Point> ;
  rdfs:subClassOf geo:Point ;
  rdfs:subClassOf <http://www.w3.org/ns/locn#Geometry> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
      owl:onProperty tern-loc:altitude ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
      owl:onProperty tern-loc:elevation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
      owl:onProperty tern-loc:latitude ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://linked.data.gov.au/def/datatype/QuantitativeMeasure> ;
      owl:onProperty tern-loc:longitude ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-loc:wktLiteral ;
      owl:onProperty geosparql:asWKT ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty geosparql:asWKT ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-loc:latitude ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-loc:longitude ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-loc:altitude ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-loc:elevation ;
    ] ;
.
tern-loc:Polygon
  a rdfs:Class ;
  a owl:Class ;
  rdfs:label "Polygon" ;
  rdfs:subClassOf <http://www.opengis.net/ont/sf#Polygon> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-loc:wktLiteral ;
      owl:onProperty geosparql:asWKT ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty geosparql:asWKT ;
    ] ;
.
tern-loc:altitude
  a owl:ObjectProperty ;
  rdfs:label "altitude" ;
  rdfs:subPropertyOf geo:alt ;
.
tern-loc:elevation
  a owl:ObjectProperty ;
  rdfs:label "elevation" ;
  rdfs:subPropertyOf schema:elevation ;
.
tern-loc:isLocationOf
  a owl:ObjectProperty ;
  rdfs:comment "Relate a Location to a thing." ;
  rdfs:domain tern-loc:Location ;
  rdfs:label "is location of" ;
  owl:inverseOf tern-loc:location ;
.
tern-loc:latitude
  a owl:ObjectProperty ;
  rdfs:label "latitude" ;
  rdfs:subPropertyOf geo:lat ;
.
tern-loc:location
  a owl:ObjectProperty ;
  rdfs:comment "Relate a thing to a Location." ;
  rdfs:label "location" ;
  rdfs:range tern-loc:Location ;
  rdfs:subPropertyOf <http://www.w3.org/ns/locn#location> ;
  owl:inverseOf tern-loc:isLocationOf ;
.
tern-loc:longitude
  a owl:ObjectProperty ;
  rdfs:label "longitude" ;
  rdfs:subPropertyOf geo:long ;
.
tern-loc:wktLiteral
  a rdfs:Datatype ;
  rdfs:comment "A Well-known Text serialization of a geometry object." ;
  rdfs:label "wkt literal" ;
  rdfs:subClassOf geosparql:wktLiteral ;
  skos:editorialNote "Created wktLiteral in the tern-loc namespace because there are issues with loading in geosparql:wktLiteral into Virtuoso version 07.20.3230 (latest at the time of creation)." ;
.
<https://w3id.org/tern/resources/a083902d-d821-41be-b663-1d7cb33eea66>
  schema:alternateName "TERN" ;
  schema:name "Terrestrial Ecosystem Research Network" ;
  schema:url <https://www.tern.org.au/> ;
.
